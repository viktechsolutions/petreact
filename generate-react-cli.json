{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/pages/TemplateName.tsx",
        "style": "templates/pages/TemplateName.module.scss",
        "index": "templates/pages/index.ts"
      },
      "path": "src/pages",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "withIndex": true
    },
    "widget": {
      "customTemplates": {
        "component": "templates/widgets/TemplateName.tsx",
        "style": "templates/widgets/TemplateName.module.scss",
        "index": "templates/widgets/index.ts"
      },
      "path": "src/widgets",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "withIndex": true
    },
    "shared": {
      "customTemplates": {
        "component": "templates/widgets/TemplateName.tsx",
        "style": "templates/widgets/TemplateName.module.scss"
      },
      "path": "src/shared/ui",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}